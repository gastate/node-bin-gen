{
  "name": "node-builder",
  "version": "0.0.0",
  "description": "Generate a node binary package",
  "author": "Shad Sterling <ssterling3@gsu.edu>",
  "contributors": [ "Aria Stewart <aredridel@dinhe.net>", "Shad Sterling <ssterling3@gsu.edu>" ],
  "main": "index.js",
  "bin": "index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/gastate/node-builder"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3"
  },
  "dependencies": {
    "end-of-stream": "^1.4.1",
    "make-fetch-happen": "^3.0.0",
    "pump": "^3.0.0",
    "rimraf": "^2.6.2",
    "verror": "^1.9.0",
    "yargs": "^11.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "valid-typeof": "error",
      "no-undef": "error",
      "no-unused-vars": "error",
      "no-process-exit": 0,
      "no-shadow": 0,
      "strict": 0,
      "eqeqeq": 0,
      "curly": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
